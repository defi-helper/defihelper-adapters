{
  "name": "defihelper-adapters",
  "version": "1.0.0",
  "description": "",
  "main": "./develop/server.js",
  "scripts": {
    "automates:lint:sol": "solhint 'automates/**/*.sol' && prettier -c 'automates/**/*.sol'",
    "automates:lint:sol:fix": "prettier --write 'automates/**/*.sol'",
    "automates:build": "hardhat compile",
    "automates:test": "hardhat test",
    "automates:deploy": "hardhat deploy --deploy-scripts ./automates/deploy --tags Automate,GovernanceOwner",
    "automates:etherscan-verify": "scripts/etherscan-verify.sh",
    "automates:waves:build": "node ./scripts/ride-compile.js",
    "adapters:build": "rollup -c adapters.rollup.js",
    "adapters:build-dev": "rollup -c adapters.rollup.js",
    "adapters:watch": "rollup -c adapters.rollup.js -w",
    "server": "node ./develop/server.js",
    "client:build": "parcel build ./develop/client/index.js -d public/dist --no-cache",
    "client:watch": "parcel watch ./develop/client/index.js -d public/dist",
    "dev": "npm-run-all --parallel adapters:watch client:watch server"
  },
  "author": "DeFi Helper",
  "license": "",
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.9",
    "@babel/preset-react": "^7.14.5",
    "@ethereum-waffle/mock-contract": "^3.3.0",
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@waves.exchange/provider-web": "^1.1.1",
    "@waves/signer": "^1.0.3",
    "dotenv": "^8.6.0",
    "express": "^4.17.1",
    "hardhat": "^2.5.0",
    "husky": "^7.0.0",
    "lint-staged": "^11.0.0",
    "mocha": "^7.2.0",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.5",
    "prettier": "^2.3.1",
    "prettier-plugin-solidity": "^1.0.0-beta.17",
    "remarkable": "^2.0.1",
    "remarkable-react": "^1.4.3",
    "solc": "^0.8.6",
    "solhint": "^3.3.6",
    "truffle-assertions": "^0.9.2"
  },
  "dependencies": {
    "@chainlink/contracts": "^0.2.2",
    "@defihelper/ethers-multicall": "^0.3.2",
    "@defihelper/networks": "^1.0.1",
    "@openzeppelin/contracts": "^4.2.0",
    "@waves/ride-js": "^2.1.4",
    "@waves/waves-transactions": "^4.2.5-beta.1",
    "axios": "^0.21.1",
    "bignumber.js": "^8.1.1",
    "command-line-args": "^5.2.0",
    "dayjs": "^1.10.5",
    "ethers": "^5.5.1",
    "hardhat-deploy": "^0.9.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-json-view": "^1.21.3",
    "react-router-dom": "^5.2.0",
    "rollup": "^2.55.1",
    "tiny-glob": "^0.2.9"
  },
  "lint-staged": {
    "*": "prettier --write ."
  }
}
